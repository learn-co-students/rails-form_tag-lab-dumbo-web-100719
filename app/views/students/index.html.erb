<div>
  <% @students.each do |student| %>
    <div><%= link_to student.to_s, student_path(student) %></div>
  <% end %>
</div>
<br/>
<div>
  <!-- Buttons are not meant for Get request, but you can add a method: to use it-->
  <%= button_to "Create New Student", new_student_path, method: :get %>

  <%= link_to "Create New Student", new_student_path %>
</div>